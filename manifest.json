{
  "manifest_version": 2,

  "name": "xxxtension",
  "description": "makes memes",
  "version": "1.0.0",

  "icons": {
    "16": "./img/icon.png",
    "48": "./img/icon.png",
    "128": "./img/icon.png"
  },
  
  "browser_action": {
    "default_icon": "./img/icon.png",
    "default_title": "mememachine",
    "default_popup": "popup.html"
  },
  "background": {
  "persistent": true,
  "scripts": [
    "lib/jquery-3.4.1.min.js",
    "background.js",
    "popup.js"
  ]
}
  

}
